{
  "manifest_version": 2, 
  "name": "Focus", 
  "version": "1.0", 
  "description": "Chrome Extention for Focusing in.",
  "icons": { "16": "mtn.png",
           "48": "mtn.png",
          "128": "mtn.png" },
    "content_scripts": [ 
        {
            "matches": ["https://www.youtube.com/*"], 
            "js": ["jquery.min.js", "./main.js"], 
            "css": ["./index.css"]
        }
    ], 
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "A popup will come here."
    }   
}

  